<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cskaoyan.mapper.mall.StoragesMapper">
    <insert id="uploadPic">
        insert into cskaoyan_mall_storage
        (id,`key`,name,type,size,url,add_time,update_time,deleted)
        values (#{storage.id},#{storage.key},#{storage.name},
        #{storage.type},#{storage.size},#{storage.url},#{storage.addTime},
        #{storage.updateTime},#{storage.deleted})
    </insert>
</mapper>